{
    "@@type": "DecodeLayer",
    "id": "biomass-density",
    "source": {
      "type": "raster",
      "tiles": [
        "https://storage.googleapis.com/wri-public/biomass/global/2017/v2/30/{z}/{x}/{y}.png"      ],
      "maxzoom": 12,
      "minzoom": 2
    },
    "opacity": {
      "o": "@@#params.opacity",
      "@@function": "setOpacity"
    },
    "visibility": {
      "v": "@@#params.visibility",
      "type": "deckgl",
      "@@function": "setVisibility"
    }
  }
    